  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DiagnosisGeneral  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DiagnosisGeneral">
    <xsd:annotation>
      <xsd:documentation>Element may be used only in the long-term care setting, where there are established processes supporting the sharing of diagnosis information between a resident’s facility and pharmacy. Note: The medication information enables communication of prescription-specific diagnosis information only—appropriate for use in new prescription messaging, but insufficient to support management of a resident’s full drug regimen. The intent is to convey the diagnosis that the prescribed medication is for.  There is also a need to convey diagnosis information in the Census event.  The difference here is that there is no related drug.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="SourceOfInformation" type="ecl:SourceOfInformationCode">
        <xsd:annotation>
          <xsd:documentation>Sender indicates where the sender received the diagnosis information if known. 
The source of information is the ultimate source. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>When this particular diagnosis was determined for the patient and when it was no longer active – if known. Value 07 Effective Date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>When this particular diagnosis was determined for the patient and when it was no longer active – if known. Value 36 Expiration Date</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProblemType" type="datatypes:SNOMEDCode">
        <xsd:annotation>
          <xsd:documentation>Describes the type of problem that exists for this patient. Value LD</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ProblemNameCoded" type="datatypes:ProblemNameCode">
        <xsd:annotation>
          <xsd:documentation>Identifies the Diagnosis for this patient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DiagnosisRestricted  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DiagnosisRestricted">
    <xsd:complexContent>
      <xsd:restriction base="structures:Diagnosis">
        <xsd:sequence>
          <xsd:element name="ClinicalInformationQualifier" type="ecl:DiagnosisClinicalInformationQualifierCode"/>
          <xsd:element name="Primary" type="datatypes:DiagnosisQualifiedCodeWithDescriptionRestricted"/>
          <xsd:element minOccurs="0" name="Secondary" type="datatypes:DiagnosisQualifiedCodeWithDescriptionRestricted"/>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DigitalSignature  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DigitalSignature">
    <xsd:annotation>
      <xsd:documentation>The DigitalSignature element is used for controlled substances for MedicationPrescribed information in NewRx, RxChangeResponse and RxRenewalResponse transactions as these are “fillable” prescription transactions per the regulation. Transactions that are not fillable are not required to be signed.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:sequence maxOccurs="2">
          <xsd:annotation>
            <xsd:documentation>Repeats twice to allow transitioning to a new hashing method.</xsd:documentation>
          </xsd:annotation>
          <xsd:element name="DigestMethod" type="ecl:DigestMethodCode">
            <xsd:annotation>
              <xsd:documentation>Defines the Hashing method, eg. SHA-1 or SHA-256 to be used.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="DigestValue" type="datatypes:an">
            <xsd:annotation>
              <xsd:documentation>This field is used to confirm that all fields have been included in the digital signature. Apply a SHA1 Hash and base64 encode the result.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="SignatureValue" type="datatypes:an">
            <xsd:annotation>
              <xsd:documentation>DigestValue that has been signed with a Private Key, base64 encode the results.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="X509Data" type="datatypes:an">
            <xsd:annotation>
              <xsd:documentation>Base64 encoded raw bytes of the X509 certificate (which contains the Public Key). Used by the receiver to validate the digital signature.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
        <xsd:element name="DigitalSignatureIndicator" type="xsd:boolean">
          <xsd:annotation>
            <xsd:documentation>True/False indicating the prescription has been digitally signed.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
    </xsd:sequence>
    <xsd:attribute name="Version" type="datatypes:an" use="required">
      <xsd:annotation>
        <xsd:documentation>The digital signature version used.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Dosage  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Dosage">
    <xsd:sequence>
      <xsd:element name="DoseQuantity" type="datatypes:n"/>
      <xsd:sequence minOccurs="0">
        <xsd:element name="DoseRangeModifier" type="datatypes:TOOR">
          <xsd:annotation>
            <xsd:documentation>Not dependent on another field such as a body metric but it may be related to indication (as needed for pain).</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="DoseQuantity" type="datatypes:n"/>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DoseAdministration  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DoseAdministration">
    <xsd:annotation>
      <xsd:documentation>Specified - remaining fields populated. When DoseAdministration is present, it SHALL contain One Delivery Method, One Dose, One Form, and One Route. As appropriate, elements may repeat for multiple instructions or ranges etc.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:sequence>
        <xsd:element name="DoseDeliveryMethod" type="datatypes:SigCode">
          <xsd:annotation>
            <xsd:documentation>Examples: 
take, apply, swish, swallow, inject, insert, chew, use, give, sprinkle, mix, dissolve…</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" name="DoseDeliveryMethodModifier" type="datatypes:SigCode">
          <xsd:annotation>
            <xsd:documentation>Supports the use of modifiers such as sparingly, frequently, repeatedly, gently, lightly.	 When needed to construct with Vehicle, examples include: with, in, on. The Dose Delivery Method Modifier provides information as to how the medication is to be delivered via the vehicle, i.e. mixed in, sprinkled on, etc.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:choice>
        <xsd:element name="Dosage" type="structures:Dosage"/>
        <xsd:element name="DoseCalculation" type="structures:DoseCalculation">
          <xsd:annotation>
            <xsd:documentation>This field may often be used in conjunction with the Rate fields within the Timing Segment and/or the Vehicle Segment. Primary use will be in non-retail settings, i.e. hospital, acute care, long term care, home care.  An example using a dose calculation: Cyclophosphamide:  600mg/meter squared in 250ml Normal Saline over 1-2 hours.  Repeat every 2 weeks.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:sequence>
          <xsd:element name="DoseAmount" type="datatypes:SigCode">
            <xsd:annotation>
              <xsd:documentation>Examples such as thin ribbon, pea sized amount, enough to cover, enough to fill, liberal amount, small amount</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DoseClarifyingFreeText" type="datatypes:an1..255"/>
        </xsd:sequence>
      </xsd:choice>
      <xsd:element name="DoseUnitOfMeasure" type="datatypes:DoseUnitOfMeasureCode">
        <xsd:annotation>
          <xsd:documentation>See NCPDP External Code List for code set.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Vehicle" type="structures:Vehicle">
        <xsd:annotation>
          <xsd:documentation>If the vehicle has a product or ingredient code, the appropriate product code element is used.

The DoseDeliveryMethodModifier provides information as to how the medication is to be delivered via the vehicle, i.e. mixed in, sprinkled on, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="RouteOfAdministration" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="RouteOfAdministrationClarifyingFreeText" type="datatypes:an1..255"/>
      <xsd:sequence minOccurs="0">
        <xsd:element name="SiteOfAdministration" type="datatypes:SigCode"/>
        <xsd:element minOccurs="0" name="SiteOfAdministrationClarifyingFreeText" type="datatypes:an1..255"/>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DoseCalculation  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DoseCalculation">
    <xsd:annotation>
      <xsd:documentation>600mg/meter squared in 250ml Normal Saline over 1-2 hours.  Repeat every 2 weeks</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DosingBasisNumeric" type="datatypes:n"/>
      <xsd:sequence minOccurs="0">
        <xsd:element name="DosingBasisRangeModifier" type="datatypes:TOOR"/>
        <xsd:element name="DosingBasisNumeric" type="datatypes:n"/>
      </xsd:sequence>
      <xsd:element name="DosingBasisUnitOfMeasure" type="datatypes:SigCode">
        <xsd:annotation>
          <xsd:documentation>Examples such as milligram per kilogram, milligram per meter squared</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:element minOccurs="0" name="BodyMetricQualifier" type="datatypes:BodyMetricQualifier"/>
        <xsd:element minOccurs="0" name="BodyMetricValue" type="datatypes:n1..18"/>
      </xsd:sequence>
      <xsd:sequence minOccurs="0">
        <xsd:element name="CalculatedDoseNumeric" type="datatypes:n">
          <xsd:annotation>
            <xsd:documentation>The Calculated Dose Numeric Value can be populated with either the information as specified by the prescriber or by performing the calculation using the  can be determined either by the prescribing system sending the field and/or by performing the calculation based on the Dosing Basis and Body Metric elements.  For example, if the Dosing  Basis Numeric Value is 600 and the Dosing Basis Numeric Value Text is milligrams per kilogram, the prescriber’s system may send the patient’s weight in Body Metric Value and then populate the result of the calculation in this field.  Alternatively, the prescriber’s system may send the patient’s weight in Body Metric Value and allow the pharmacist to use that information to calculate/validate the dose.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="CalculatedDoseUnitOfMeasure" type="datatypes:SigCode">
          <xsd:annotation>
            <xsd:documentation>Examples such as milligram per day, microgram per hour.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:element minOccurs="0" name="DoseCalculationClarifyingFreeText" type="datatypes:an1..255"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DosingValueLocationComposite  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DosingValueLocationComposite">
    <xsd:sequence>
      <xsd:element name="Value" type="datatypes:n"/>
      <xsd:element maxOccurs="9" minOccurs="0" name="DosingLocationType" type="datatypes:an"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DrugAdministrationMedication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DrugAdministrationMedication">
    <xsd:complexContent>
      <xsd:restriction base="structures:MedicationAbstract">
        <xsd:sequence>
          <xsd:element name="DrugDescription" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Quantity" type="datatypes:QuantityForPrescribed"/>
          <xsd:element minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="WrittenDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="LastFillDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DateValidated" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Substitutions" type="ecl:SubstitutionCodeRestricted">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="2" minOccurs="0" name="Diagnosis" type="structures:DiagnosisRestricted"/>
          <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
          <xsd:element minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Note" type="datatypes:an1..210">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="5" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="5" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Sig" type="structures:Sig"/>
          <xsd:element maxOccurs="25" minOccurs="0" name="Compound" type="datatypes:CompoundIngredientLotNotUsed">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="SoldDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Pharmacy" type="structures:Pharmacy">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Prescriber" type="structures:PrescriberGeneralChoice"/>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
            <xsd:annotation>
              <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
            <xsd:annotation>
              <xsd:documentation>The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>Name of the manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
          <xsd:element minOccurs="0" name="RxFillIndicator" type="ecl:RxFillIndicatorType"/>
          <xsd:element minOccurs="0" name="Service" type="structures:ServiceStructure"/>
          <xsd:element minOccurs="0" name="IVAdministration" type="structures:IVAdministrationType"/>
          <xsd:element minOccurs="0" name="SelfAdministrationAllowed" type="ecl:BooleanCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="AnticipatedDischargeDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CurrentTreatmentCycle" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="NumberOfCyclesPlanned" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Duration  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Duration">
    <xsd:sequence>
      <xsd:element name="DurationNumericValue" type="datatypes:n"/>
      <xsd:element name="DurationText" type="datatypes:SigCode"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  DurationTriggerTiming  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="DurationTriggerTiming">
    <xsd:sequence>
      <xsd:element name="Trigger" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="DurationClarifyingFreeText" type="datatypes:an1..255"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Facility  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Facility">
    <xsd:annotation>
      <xsd:documentation>The Facility composite may be included to describe the care facility or other location at which the patient resides.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Identification" type="datatypes:FacilityID"/>
      <xsd:element minOccurs="0" name="FacilityName" type="datatypes:an1..70"/>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  FillStatus  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="FillStatus">
    <xsd:choice>
      <xsd:element name="Dispensed" type="datatypes:NoteTypeWithReasonCode18">
        <xsd:annotation>
          <xsd:documentation>Value AH</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PartiallyDispensed" type="datatypes:NoteTypeWithReasonCode18">
        <xsd:annotation>
          <xsd:documentation>Value AJ.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NotDispensed" type="datatypes:NoteTypeWithReasonCode">
        <xsd:annotation>
          <xsd:documentation>Value AK</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Transferred" type="datatypes:NoteType"/>
    </xsd:choice>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  FinancialGuarantorType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="FinancialGuarantorType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="datatypes:NameType">
        <xsd:annotation>
          <xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="BusinessName" type="datatypes:an1..70">
        <xsd:annotation>
          <xsd:documentation>When pharmacist is not associated with a pharmacy (see Pharmacy element) and a separate business name is needed.
			</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
        <xsd:annotation>
          <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
        <xsd:annotation>
          <xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
        <xsd:annotation>
          <xsd:documentation>See PatientFull for Definition and Usage</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FinancialGuarantorRelationship" type="ecl:EligibilityPatientRelationshipCode">
        <xsd:annotation>
          <xsd:documentation>Code indicating the relationship between the financial guarantor and the patient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  FreeTextTypeBasic  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="FreeTextTypeBasic">
    <xsd:sequence>
      <xsd:element default="Y" name="IsFreeText" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>A value of "Y" to indicate the answer to the question is free text. A value of "N" should not be used. Use other question type if not free text.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
