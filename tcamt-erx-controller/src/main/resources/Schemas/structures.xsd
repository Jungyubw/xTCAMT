          <xsd:element maxOccurs="5" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="5" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Sig" type="structures:Sig">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="25" minOccurs="0" name="Compound" type="datatypes:CompoundIngredientLotNotUsed">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="SoldDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Pharmacy" type="structures:Pharmacy">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Prescriber" type="structures:PrescriberGeneralChoice"/>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
            <xsd:annotation>
              <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
            <xsd:annotation>
              <xsd:documentation> The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>Name of the manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
          <xsd:element maxOccurs="0" minOccurs="0" name="RxFillIndicator" type="ecl:RxFillIndicatorType"/>
          <xsd:element maxOccurs="0" minOccurs="0" name="OfficeOfPharmacyAffairsID" type="datatypes:an1..15">
            <xsd:annotation>
              <xsd:documentation>Health Resources and Services Administration (HRSA) Office of Pharmacy Affairs (OPA) Identification Code associated with the eligibility of this prescription for drugs purchased pursuant to rights under Section 340B of the Public Health Service Act of 1992.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="InjuryRelated" type="ecl:InjuryRelatedCode"/>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Prescriber  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Prescriber">
    <xsd:annotation>
      <xsd:documentation>Value PC</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:OptionalPrescriber">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:NonVeterinarianID">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
            <xsd:annotation>
              <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PrescriberAgent" type="structures:NameComposite"/>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PrescriberChoice  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PrescriberChoice">
    <xsd:complexContent>
      <xsd:restriction base="structures:HistoryPrescriberChoice">
        <xsd:choice>
          <xsd:element name="NonVeterinarian" type="structures:Prescriber"/>
          <xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorPrescriber"/>
        </xsd:choice>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PrescriberGeneral  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PrescriberGeneral">
    <xsd:complexContent>
      <xsd:restriction base="structures:HistoryPrescriber">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="Identification" type="datatypes:NonVeterinarianID">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
            <xsd:annotation>
              <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PrescriberAgent" type="structures:NameComposite"/>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PrescriberGeneralChoice  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PrescriberGeneralChoice">
    <xsd:complexContent>
      <xsd:restriction base="structures:HistoryPrescriberChoice">
        <xsd:choice>
          <xsd:element name="NonVeterinarian" type="structures:PrescriberGeneral"/>
          <xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorPrescriberGeneral"/>
        </xsd:choice>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PriorPeriodCorrectionType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PriorPeriodCorrectionType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CorrectionDate" type="datatypes:StartEndDateType"/>
      <xsd:element minOccurs="0" name="CorrectionType" type="datatypes:an"/>
      <xsd:element maxOccurs="unbounded" name="BenefitsCoordination" type="structures:BenefitsCoordinationForCensus"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Provider  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Provider">
    <xsd:annotation>
      <xsd:documentation>Used when medical provider is needed, not the prescriber. Designated Provider - The patient’s provider for which this service applies, if applicable. The designated provider may be the primary care provider, the medical home provider, the facility provider or other provider. This information is provided as reference.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Identification" type="datatypes:NonVeterinarianID"/>
      <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10"/>
      <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
        <xsd:annotation>
          <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Name" type="datatypes:NameType"/>
      <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
        <xsd:annotation>
          <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionDetailCommonType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionDetailCommonType">
    <xsd:sequence>
      <xsd:element name="QuestionID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the payer to identify the question within the question set. Must be unique per question in the transaction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SequenceNumber" type="datatypes:n1..3">
        <xsd:annotation>
          <xsd:documentation>Sequence number assigned by the payer to indicate the order of the question in relation to other questions in the question set. Must be unique per question in the transaction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="QuestionLevel" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>Question level to indicate grouping of questions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="QuestionText" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Question text to display to the provider.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="CodedReference" type="structures:CodedReference"/>
      <xsd:element minOccurs="0" name="DefaultNextQuestionID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>A QuestionID or value of "END" to identify the next question when it is not dependent on the answer to the question. Required when the QuestionType is date or free text or when the QuestionType is select or numeric and the NextQuestionID for the answer choices or numeric conditions are not provided.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionDetailForRequestType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionDetailForRequestType">
    <xsd:complexContent>
      <xsd:extension base="structures:QuestionDetailCommonType">
        <xsd:sequence>
          <xsd:element name="QuestionType" type="structures:QuestionTypeForRequest"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionDetailForResponseType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionDetailForResponseType">
    <xsd:complexContent>
      <xsd:extension base="structures:QuestionDetailCommonType">
        <xsd:sequence>
          <xsd:element name="QuestionType" type="structures:QuestionType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionSetGroup  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionSetGroup">
    <xsd:sequence>
      <xsd:element name="Header" type="structures:QuestionSetHeader"/>
      <xsd:element maxOccurs="unbounded" name="Question" type="structures:QuestionDetailForResponseType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionSetGroupForRequest  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionSetGroupForRequest">
    <xsd:sequence>
      <xsd:element name="Header" type="structures:QuestionSetHeaderWithComment"/>
      <xsd:element maxOccurs="unbounded" name="Question" type="structures:QuestionDetailForRequestType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionSetGroupForResponse  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionSetGroupForResponse">
    <xsd:sequence>
      <xsd:element name="Header" type="structures:QuestionSetHeader"/>
      <xsd:element maxOccurs="unbounded" name="Question" type="structures:QuestionDetailForResponseType"/>
      <xsd:element name="DeadlineForReply" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionSetHeader  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionSetHeader">
    <xsd:sequence>
      <xsd:element name="QuestionSetID" type="datatypes:an1..70">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the payer to identify the question set. It should not be assumed this is a unique identifier over time or across payers.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="QuestionSetTitle" type="datatypes:an1..70">
        <xsd:annotation>
          <xsd:documentation>Title of the question set.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="QuestionSetDescription" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Descriptive information about the question set.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="QuestionSetContactCommunicationNumber" type="datatypes:CommunicationNumbersTypeRestricted"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionSetHeaderWithComment  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionSetHeaderWithComment">
    <xsd:complexContent>
      <xsd:extension base="structures:QuestionSetHeader">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="QuestionSetComment" type="datatypes:an1..2000">
            <xsd:annotation>
              <xsd:documentation>The vendor should always allow the provider to send comments in addition to answering the questions.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionType  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionType">
    <xsd:choice>
      <xsd:element name="Select" type="structures:PASelectType">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Date" type="datatypes:DateTypeFlag">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Numeric" type="structures:NumericTypeBasic">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FreeText" type="structures:FreeTextTypeBasic">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  QuestionTypeForRequest  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="QuestionTypeForRequest">
    <xsd:choice>
      <xsd:element name="Select" type="structures:PASelectTypeForRequest">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is to be selected from a set of choices.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Date" type="structures:DateTypeForRequest">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is a date.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Numeric" type="structures:NumericChoiceForPARequest">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is numeric.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FreeText" type="structures:AnswerForTextType">
        <xsd:annotation>
          <xsd:documentation>Indicates the answer to the question is FreeText.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  REMSApprovedDetail  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="REMSApprovedDetail">
    <xsd:sequence>
      <xsd:element name="REMSCaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="REMSAuthorizationNumber" type="datatypes:an1..140">
        <xsd:annotation>
          <xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AuthorizationPeriod" type="structures:DateGroupWithEffectiveExpiredDate"/>
      <xsd:element minOccurs="0" name="REMSNote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  REMSDeniedType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="REMSDeniedType">
    <xsd:sequence>
      <xsd:element name="REMSCaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="REMSNote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  REMSModelType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="REMSModelType">
    <xsd:sequence>
      <xsd:element name="SolicitedModel" type="structures:REMSSolicitedType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  REMSOpenType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="REMSOpenType">
    <xsd:sequence>
      <xsd:element name="REMSCaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DeadlineForReply" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="QuestionSet" type="structures:QuestionSetGroup"/>
      <xsd:element minOccurs="0" name="REMSNote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  REMSOpenTypeResponse  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="REMSOpenTypeResponse">
    <xsd:sequence>
      <xsd:element name="REMSCaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MoreInformationRequired" type="structures:QuestionSetGroupForResponse"/>
      <xsd:element minOccurs="0" name="REMSNote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  REMSReasonClosedCodeType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="REMSReasonClosedCodeType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="REMSCaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the REMS Administrator to identify the specific case.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
