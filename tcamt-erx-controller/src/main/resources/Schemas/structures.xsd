  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  HistoryPrescriber  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="HistoryPrescriber">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Identification" type="datatypes:NonVeterinarianID">
        <xsd:annotation>
          <xsd:documentation>One occurrence is the prescriber number. One occurrence may be used to identify the clinic. At least one occurrence must contain the Prescriber individual (not organizational) NPI Number. DEANumber is sent when applicable.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
        <xsd:annotation>
          <xsd:documentation>Value A for the NUCC taxonomy list.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
        <xsd:annotation>
          <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Name" type="datatypes:NameType"/>
      <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
        <xsd:annotation>
          <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element minOccurs="0" name="PrescriberAgent" type="structures:NameComposite"/>
      <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  HistoryPrescriberChoice  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="HistoryPrescriberChoice">
    <xsd:choice>
      <xsd:element name="NonVeterinarian" type="structures:HistoryPrescriber"/>
      <xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorHistoryPrescriber"/>
    </xsd:choice>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  IVAdministrationType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="IVAdministrationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="NumberOfLumens" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Indicates the number of lumens in the IV access line that will be used for administering the medication.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DiluentAmount" type="datatypes:QuantityWithoutSecondQuantity">
        <xsd:annotation>
          <xsd:documentation>Indicates the amount of diluent (which will have a separate order) to be used for administration. Concepts of the intended or actual diluent amount unit of measure.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SpecificAdministrationGauge" type="datatypes:n">
        <xsd:annotation>
          <xsd:documentation>Indicates the size (gauge) of the needle or catheter being used for medication administration, i.e. 14, 18, 20.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SpecificAdministrationBrand" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Indicates the specific Brand of the PICC line used for central IV access.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SpecificAdministrationLength" type="datatypes:an">
        <xsd:annotation>
          <xsd:documentation>Indicates the size (length) of the PICC line used for central IV access.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SpecificAdministrationPump" type="ecl:BooleanCode"/>
      <xsd:element name="IVAccessType" type="datatypes:SNOMEDCode"/>
      <xsd:element minOccurs="0" name="IVAccessDeviceType">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="IVAccessDeviceTypeDescription" type="datatypes:an">
              <xsd:annotation>
                <xsd:documentation>Indicates the specific device used for central IV access, if applicable i.e. implanted port, PICC line, tunneled line, non-tunneled line.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="IVAccessDeviceType" type="datatypes:SNOMEDCode">
              <xsd:annotation>
                <xsd:documentation>The code representing the IVAccessDeviceType. It is the intended specific device used for central IV access.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="IVAccessCatheterTip">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="IVAccessCatheterTipDescription" type="datatypes:an">
              <xsd:annotation>
                <xsd:documentation>Indicates the specific structure of the catheter tip for the specific device used for central or midline IV access., if applicable i.e. valved, non-valved.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="IVAccessCatheterTipType" type="datatypes:SNOMEDCode">
              <xsd:annotation>
                <xsd:documentation>The code representing the IVAccessCatheterTipType. It is the intended specific structure of the catheter tip for the specific device used for central or midline IV access.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="IVInfusion">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="IVInfusionDescription" type="datatypes:an">
              <xsd:annotation>
                <xsd:documentation>Indicates the intended type of IV infusion i.e. continuous, intermittent, TOTAL perenteral nutrition (TPN), PARTIAL perenteral nutrition (PPN), maintenance.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="IVInfusionType" type="datatypes:SNOMEDCode">
              <xsd:annotation>
                <xsd:documentation>The code representing the IVInfusionTypeType. It is the intended type of IV infusion.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Indication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Indication">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="IndicationPrecursor" type="datatypes:SigCode">
        <xsd:annotation>
          <xsd:documentation>Examples such as As needed, as directed, when, for…</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Indication" type="datatypes:SigCode">
        <xsd:annotation>
          <xsd:documentation>Examples such as Blood Pressure, Finger Stick Blood Glucose</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:element name="IndicationValue" type="datatypes:SigCode">
          <xsd:annotation>
            <xsd:documentation>Examples such as equal than, greater than, less than</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" name="IndicationVariableModifier" type="datatypes:ANDORTO"/>
        <xsd:element minOccurs="0" name="IndicationValue" type="datatypes:SigCode"/>
      </xsd:sequence>
      <xsd:element minOccurs="0" name="IndicationValueUnit" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="IndicationValueUnitOfMeasure" type="datatypes:SigCode"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Instruction  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Instruction">
    <xsd:choice>
      <xsd:sequence>
        <xsd:element name="InstructionIndicator" type="ecl:InstructionIndicatorCodes">
          <xsd:annotation>
            <xsd:documentation>When structured Sig is as directed based on medical encounter.
Skip Dose. (Timing is situational.)</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:sequence maxOccurs="49" minOccurs="0">
          <xsd:element minOccurs="0" name="MultipleTimingModifier" type="datatypes:ANDOR">
            <xsd:annotation>
              <xsd:documentation>May be used to linked to any of the timing methods -– Administration Timing and/or Frequency and/or Interval.  For example,” take each morning, before bed and every four hours as needed.”</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="TimingAndDuration" type="structures:TimingAndDuration"/>
        </xsd:sequence>
      </xsd:sequence>
      <xsd:sequence>
        <xsd:choice>
          <xsd:element name="AdministrationIndicator" type="ecl:AdministrationIndicatorCodes">
            <xsd:annotation>
              <xsd:documentation>When structured Sig is as directed per manufacturer's packaging.
Skip Dose.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:sequence>
            <xsd:element name="DoseAdministration" type="structures:DoseAdministration"/>
            <xsd:element name="TimingAndDuration" type="structures:TimingAndDuration">
              <xsd:annotation>
                <xsd:documentation>This element is used to provide instruction about the timing of the Sig -– when/how often/at what rate/for how long -– the medication is to be taken. Specific time fields must be Consistent with ISO format.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:sequence maxOccurs="49" minOccurs="0">
              <xsd:element minOccurs="0" name="MultipleTimingModifier" type="datatypes:ANDOR">
                <xsd:annotation>
                  <xsd:documentation>May be used to link any of the timing methods – Administration Timing and/or Frequency and/or Interval.  For example,take each morning, before bed and every four hours as needed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="TimingAndDuration" type="structures:TimingAndDuration"/>
            </xsd:sequence>
          </xsd:sequence>
        </xsd:choice>
        <xsd:sequence minOccurs="0">
          <xsd:element maxOccurs="50" name="IndicationForUse" type="structures:Indication">
            <xsd:annotation>
              <xsd:documentation>Defines the indication for use of the medication as meant to be conveyed to the patient. Note:  The indication for use of the medication as meant to be conveyed to the patient may differ from the clinical indication for use.  As an example, take for sleep may be conveyed to the patient; the clinical indication may be insomnia.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="IndicationClarifyingFreeText" type="datatypes:an1..255"/>
        </xsd:sequence>
        <xsd:element maxOccurs="50" minOccurs="0" name="MaximumDoseRestriction" type="structures:MaximumDoseRestriction">
          <xsd:annotation>
            <xsd:documentation>Defines a maximum or dose limit, as specified by the prescriber. The value entered may or may not match with the information provided by clinical decision support tools.  Professional judgment should be used when discrepancies occur.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:choice>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  IntervalTiming  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="IntervalTiming">
    <xsd:sequence>
      <xsd:element name="IntervalNumericValue" type="datatypes:n"/>
      <xsd:sequence minOccurs="0">
        <xsd:element name="VariableIntervalModifier" type="datatypes:ANDORTO"/>
        <xsd:element name="IntervalNumericValue" type="datatypes:n"/>
      </xsd:sequence>
      <xsd:element name="IntervalUnits" type="datatypes:SigCode"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MandatoryAddressPharmacy  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MandatoryAddressPharmacy">
    <xsd:complexContent>
      <xsd:restriction base="structures:MandatoryPharmacy">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:PharmacyID">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Pharmacist" type="structures:Pharmacist"/>
          <xsd:element name="BusinessName" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Address" type="datatypes:MandatoryAddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MandatoryPharmacy  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MandatoryPharmacy">
    <xsd:annotation>
      <xsd:documentation>Value P2</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:Pharmacy">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:PharmacyID">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Pharmacist" type="structures:Pharmacist"/>
          <xsd:element name="BusinessName" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalPharmacy for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MandatoryPrescriber  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MandatoryPrescriber">
    <xsd:annotation>
      <xsd:documentation>Value PC</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:Prescriber">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:NonVeterinarianID">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
            <xsd:annotation>
              <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Address" type="datatypes:MandatoryAddressType">
            <xsd:annotation>
              <xsd:documentation>The prescriber address on a NEWRX must be the practice address.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PrescriberAgent" type="structures:NameComposite"/>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MandatoryPrescriberChoice  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MandatoryPrescriberChoice">
    <xsd:complexContent>
      <xsd:restriction base="structures:HistoryPrescriberChoice">
        <xsd:choice>
          <xsd:element name="NonVeterinarian" type="structures:MandatoryPrescriber"/>
          <xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorMandatoryPrescriber"/>
        </xsd:choice>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MaximumDoseRestriction  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MaximumDoseRestriction">
    <xsd:sequence>
      <xsd:element name="MaximumDoseRestrictionNumericValue" type="datatypes:n"/>
      <xsd:choice>
        <xsd:element name="MaximumDoseRestrictionForm" type="datatypes:DoseUnitOfMeasureCode"/>
        <xsd:element name="MaximumDoseRestrictionUnits" type="datatypes:SigCode"/>
      </xsd:choice>
      <xsd:element name="MaximumDoseRestrictionDurationValue" type="datatypes:n"/>
      <xsd:element name="MaximumDoseRestrictionDurationUnit" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="MaximumDoseRestrictionClarifyingFreeText" type="datatypes:an1..255"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MeasurementDuration  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MeasurementDuration">
    <xsd:sequence>
      <xsd:element name="MeasurementDurationNumericValue" type="datatypes:n"/>
      <xsd:element name="MeasurementDurationText" type="datatypes:SigCode"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MeasurementDurationTriggerTiming  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MeasurementDurationTriggerTiming">
    <xsd:sequence>
      <xsd:element name="MeasurementTrigger" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="MeasurementDurationClarifyingFreeText" type="datatypes:an1..255"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MeasurementFrequencyTiming  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MeasurementFrequencyTiming">
    <xsd:sequence>
      <xsd:element name="MeasurementFrequencyNumericValue" type="datatypes:n"/>
      <xsd:sequence minOccurs="0">
        <xsd:element name="MeasurementVariableFrequencyModifier" type="datatypes:ANDORTO">
          <xsd:annotation>
            <xsd:documentation>Used to express when there is more than one Measurement Frequency as to whether the frequencies are all required to be used (AND) or if any of the frequencies can be used (OR/TO).</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="MeasurementFrequencyNumericValue" type="datatypes:n"/>
      </xsd:sequence>
      <xsd:element name="MeasurementrequencyUnits" type="datatypes:SigCode"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MeasurementIntervalTiming  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MeasurementIntervalTiming">
    <xsd:sequence>
      <xsd:element name="MeasurementIntervalNumericValue" type="datatypes:n"/>
      <xsd:sequence minOccurs="0">
        <xsd:element name="MeasurementVariableIntervalModifier" type="datatypes:ANDORTO">
          <xsd:annotation>
            <xsd:documentation>Used to express when there is more than one Interval as to whether the intervals are all required to be used (AND) or if any of the intervals can be used (OR/TO).</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="MeasurementIntervalNumericValue" type="datatypes:n"/>
      </xsd:sequence>
      <xsd:element name="MeasurementIntervalUnits" type="datatypes:SigCode"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MeasurementTiming  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MeasurementTiming">
    <xsd:sequence>
      <xsd:sequence minOccurs="0">
        <xsd:element name="MeasurementTimingNumericValue" type="datatypes:n"/>
        <xsd:element name="VariableMeasurementTimingModifier" type="datatypes:ANDORTO">
          <xsd:annotation>
            <xsd:documentation>Used to express when there is more than one time as to whether the times are all required to be used (AND) or if any of the times can be used (OR). (TO) is used if the Measurement Timing provides for a range.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:sequence minOccurs="0">
          <xsd:element name="AdministrationTimingNumericValue" type="datatypes:n"/>
        </xsd:sequence>
        <xsd:element name="MeasurementTimingUnits" type="datatypes:SigCode"/>
      </xsd:sequence>
      <xsd:element minOccurs="0" name="MeasurementTimingModifier" type="datatypes:SigCode">
        <xsd:annotation>
          <xsd:documentation>Examples such as before, during, with, without, after, while, at., on, start, end.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MeasurementTimingEvent" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="MeasurementTimingClarifyingFreeText" type="datatypes:an1..255">
        <xsd:annotation>
          <xsd:documentation>Used to add clarity to the titration measurement timing for elements that cannot be codified.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Medication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Medication">
    <xsd:annotation>
      <xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:MedicationAbstract">
        <xsd:sequence>
          <xsd:element name="DrugDescription" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Quantity" type="datatypes:QuantityAbstract">
            <xsd:annotation>
              <xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
            <xsd:annotation>
              <xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="WrittenDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value 85.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="LastFillDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value LD.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value 36.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value 07.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value  PE.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value BE.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DateValidated" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value BE.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Substitutions" type="ecl:SubstitutionCodeRestricted">
            <xsd:annotation>
              <xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Diagnosis" type="structures:DiagnosisRestricted"/>
          <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Note" type="datatypes:an1..210">
            <xsd:annotation>
              <xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
            <xsd:annotation>
              <xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
            <xsd:annotation>
              <xsd:documentation>See Imp Guide for more information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
            <xsd:annotation>
              <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Sig" type="structures:Sig">
            <xsd:annotation>
              <xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Compound" type="datatypes:CompoundIngredient">
            <xsd:annotation>
              <xsd:documentation>For ingredient information. Sub elements loop for each ingredient.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="SoldDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
            <xsd:annotation>
              <xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
            <xsd:annotation>
              <xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
            <xsd:annotation>
              <xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
            <xsd:annotation>
              <xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Pharmacy" type="structures:OptionalPharmacy"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Prescriber" type="structures:HistoryPrescriberChoice"/>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment"/>
          <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
            <xsd:annotation>
              <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
            <xsd:annotation>
              <xsd:documentation> The prescription has been split for both mail order and retail. The retail order is only for a limited supply  until the mail order arrives.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>Name of the manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
          <xsd:element maxOccurs="0" minOccurs="0" name="RxFillIndicator" type="ecl:RxFillIndicatorType"/>
          <xsd:element minOccurs="0" name="DiabeticSupply" type="datatypes:DiabeticSupplyType">
            <xsd:annotation>
              <xsd:documentation>When sig is sent in text only, the route of administration is strongly recommended. When structured Sig is sent, the route of administration is required.  When sig is sent in text only, the frequency is strongly recommended. When structured Sig is sent, frequency is strongly recommended. When the structured Sig is used the TestingFrequency must match to the structured Sig FrequencyNumericValue. When ordering items such as test strips or lancets, the Quantity must reflect the package quantity of the item (i.e. 100 instead of 1 box).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="InjuryRelated" type="ecl:InjuryRelatedCode"/>
          <xsd:element minOccurs="0" name="Service" type="structures:ServiceStructure"/>
          <xsd:element minOccurs="0" name="IVAdministration" type="structures:IVAdministrationType"/>
          <xsd:element minOccurs="0" name="SelfAdministrationAllowed" type="ecl:BooleanCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="AnticipatedDischargeDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CurrentTreatmentCycle" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="NumberOfCyclesPlanned" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MedicationAbstract  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MedicationAbstract">
    <xsd:annotation>
      <xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DrugDescription" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>Is the self-contained full drug name, strength, and form. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Quantity" type="datatypes:QuantityAbstract">
        <xsd:annotation>
          <xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
        <xsd:annotation>
          <xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="WrittenDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 85.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LastFillDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value LD.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 36.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 07.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value  PE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DateValidated" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Substitutions" type="ecl:SubstitutionCodeRestricted">
        <xsd:annotation>
          <xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Diagnosis" type="structures:Diagnosis"/>
      <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Note" type="datatypes:an1..210">
        <xsd:annotation>
          <xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
        <xsd:annotation>
          <xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
        <xsd:annotation>
          <xsd:documentation>See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Sig" type="structures:Sig">
        <xsd:annotation>
          <xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Compound" type="datatypes:CompoundIngredient">
        <xsd:annotation>
          <xsd:documentation>For ingredient information. Sub elements loop for each ingredient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="SoldDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
        <xsd:annotation>
          <xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
        <xsd:annotation>
          <xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
        <xsd:annotation>
          <xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
        <xsd:annotation>
          <xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Pharmacy" type="structures:OptionalPharmacy"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Prescriber" type="structures:HistoryPrescriberChoice"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceAbstract"/>
      <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
        <xsd:annotation>
          <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
        <xsd:annotation>
          <xsd:documentation> The prescription has been split for both mail order and retail. The retail order is only for a limited supply  until the mail order arrives.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>Name of the manufacturer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="RxFillIndicator" type="ecl:RxFillIndicatorType"/>
      <xsd:element maxOccurs="10" minOccurs="0" name="OfficeOfPharmacyAffairsID" type="datatypes:an1..15">
        <xsd:annotation>
          <xsd:documentation>Health Resources and Services Administration (HRSA) Office of Pharmacy Affairs (OPA) Identification Code associated with the eligibility of this prescription for drugs purchased pursuant to rights under Section 340B of the Public Health Service Act of 1992.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DeliveryRequest" type="ecl:DeliveryRequestCode">
        <xsd:annotation>
          <xsd:documentation>Indicator of whether patient requests delivery of prescription.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DeliveryLocation" type="ecl:DeliveryLocationtCode">
        <xsd:annotation>
          <xsd:documentation>If patient specifies a delivery, this is the location for the delivery.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DiabeticSupply" type="datatypes:DiabeticSupplyType">
        <xsd:annotation>
          <xsd:documentation>When sig is sent in text only, the route of administration is strongly recommended. When structured Sig is sent, the route of administration is required.  When sig is sent in text only, the frequency is strongly recommended. When structured Sig is sent, frequency is strongly recommended. When the structured Sig is used the TestingFrequency must match to the structured Sig FrequencyNumericValue. When ordering items such as test strips or lancets, the Quantity must reflect the package quantity of the item (i.e. 100 instead of 1 box).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DispensedPackageMethod" type="ecl:DispensedPackageMethodCode">
        <xsd:annotation>
          <xsd:documentation>Only used in LTC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="10" minOccurs="0" name="WarningLabel" type="structures:CFWarningLabelForRxFill"/>
      <xsd:element minOccurs="0" name="Patient" type="structures:Patient"/>
      <xsd:element minOccurs="0" name="InjuryRelated" type="ecl:InjuryRelatedCode"/>
      <xsd:element minOccurs="0" name="Service" type="structures:ServiceStructure"/>
      <xsd:element minOccurs="0" name="IVAdministration" type="structures:IVAdministrationType"/>
      <xsd:element minOccurs="0" name="SelfAdministrationAllowed" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AnticipatedDischargeDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CurrentTreatmentCycle" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NumberOfCyclesPlanned" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DateRecertified" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>The date when the prescriber recertifies the medication order.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="9" minOccurs="0" name="Wound" type="structures:WoundStructure"/>
      <xsd:element minOccurs="0" name="PrescriberCheckedREMS" type="ecl:PrescriberCheckedREMSCode">
        <xsd:annotation>
          <xsd:documentation>Identifies if the prescribing system has performed an inquiry to the REMS Administrator in order to verify the REMS component of the prescription.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="REMSPatientRiskCategory" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>Identifies the risk of adverse event based on variables of the patient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="REMSAuthorizationNumber" type="datatypes:an1..140">
        <xsd:annotation>
          <xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Titration" type="structures:TitrationType"/>
      <xsd:element minOccurs="0" name="FlavoringRequested" type="ecl:YesCode">
        <xsd:annotation>
          <xsd:documentation>Indicates the prescriber is requesting flavoring be added to the prescribed product.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  MultipleDosingLocation  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="MultipleDosingLocation">
    <xsd:sequence>
      <xsd:element name="NumberOfPackagesToBeDispensed" type="datatypes:n1..2"/>
      <xsd:element maxOccurs="9" minOccurs="0" name="DosingLocationTypes" type="ecl:DosingLocationType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NameComposite  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NameComposite">
    <xsd:sequence>
      <xsd:element name="Name" type="datatypes:NameType"/>
      <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
        <xsd:annotation>
          <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NewRxPrescribedMedication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NewRxPrescribedMedication">
    <xsd:complexContent>
      <xsd:restriction base="structures:MedicationAbstract">
        <xsd:sequence>
          <xsd:element name="DrugDescription" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Quantity" type="datatypes:QuantityForPrescribed"/>
          <xsd:element minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="WrittenDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>On a NewRx the WrittenDate indicates the date the prescriber created this prescription being transmitted. It is recommended that transmission of the NewRx should be within 72 hours of the WrittenDate, with exceptions for state/federal regulations timeframe requirements. WrittenDate must precede or be equal to the transmission date. For future dating, see EffectiveDate.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="LastFillDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>EffectiveDate: The date or date/time after which this prescription being transmitted can be dispensed (i.e. do not fill before date) as authorized by the prescriber. For receipt of prescriptions with transmission of the NewRx greater than 72 hours of the WrittenDate, the RxChange transaction can be used for clarification with the prescriber. EXCEPTION: Electronic prescriptions for patients receiving Long Term Care Pharmacy Services are exempt from the EffectiveDate usage stated above.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DateValidated" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Substitutions" type="ecl:SubstitutionCodeRestricted">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="NumberOfRefills" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="2" minOccurs="0" name="Diagnosis" type="structures:Diagnosis">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
          <xsd:element minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Note" type="datatypes:an1..210">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="5" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="5" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Sig" type="structures:Sig">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="25" minOccurs="0" name="Compound" type="datatypes:CompoundIngredientLotNotUsed">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="SoldDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Pharmacy" type="structures:Pharmacy">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Prescriber" type="structures:PrescriberGeneralChoice"/>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
            <xsd:annotation>
              <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
            <xsd:annotation>
              <xsd:documentation>The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>Name of the manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
          <xsd:element minOccurs="0" name="RxFillIndicator" type="ecl:RxFillIndicatorType"/>
          <xsd:element maxOccurs="10" minOccurs="0" name="OfficeOfPharmacyAffairsID" type="datatypes:an1..15">
            <xsd:annotation>
              <xsd:documentation>Health Resources and Services Administration (HRSA) Office of Pharmacy Affairs (OPA) Identification Code associated with the eligibility of this prescription for drugs purchased pursuant to rights under Section 340B of the Public Health Service Act of 1992.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DeliveryRequest" type="ecl:DeliveryRequestCode">
            <xsd:annotation>
              <xsd:documentation>Indicator of whether patient requests delivery of prescription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DeliveryLocation" type="ecl:DeliveryLocationtCode">
            <xsd:annotation>
              <xsd:documentation>If patient specifies a delivery, this is the location for the delivery.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="DiabeticSupply" type="datatypes:DiabeticSupplyType">
            <xsd:annotation>
              <xsd:documentation>When sig is sent in text only, the route of administration is strongly recommended. When structured Sig is sent, the route of administration is required.  When sig is sent in text only, the frequency is strongly recommended. When structured Sig is sent, frequency is strongly recommended. When the structured Sig is used the TestingFrequency must match to the structured Sig FrequencyNumericValue. When ordering items such as test strips or lancets, the Quantity must reflect the package quantity of the item (i.e. 100 instead of 1 box).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="InjuryRelated" type="ecl:InjuryRelatedCode"/>
          <xsd:element minOccurs="0" name="Service" type="structures:ServiceStructure"/>
          <xsd:element minOccurs="0" name="IVAdministration" type="structures:IVAdministrationType"/>
          <xsd:element minOccurs="0" name="SelfAdministrationAllowed" type="ecl:BooleanCode"/>
          <xsd:element minOccurs="0" name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="AnticipatedDischargeDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CurrentTreatmentCycle" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="NumberOfCyclesPlanned" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="9" minOccurs="0" name="Wound" type="structures:WoundStructure"/>
          <xsd:element name="PrescriberCheckedREMS" type="ecl:PrescriberCheckedREMSCode">
            <xsd:annotation>
              <xsd:documentation>Identifies if the prescribing system has performed an inquiry to the REMS Administrator in order to verify the REMS component of the prescription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="REMSPatientRiskCategory" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>Identifies the risk of adverse event based on variables of the patient.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="REMSAuthorizationNumber" type="datatypes:an1..140">
            <xsd:annotation>
              <xsd:documentation>Number assigned by the REMS Administrator to identify an authorized transaction.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Titration" type="structures:TitrationType"/>
          <xsd:element minOccurs="0" name="FlavoringRequested" type="ecl:YesCode">
            <xsd:annotation>
              <xsd:documentation>Indicates the prescriber is requesting flavoring be added to the prescribed product.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NewRxRequestMedication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NewRxRequestMedication">
    <xsd:annotation>
      <xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DrugDescription" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded"/>
      <xsd:element minOccurs="0" name="Quantity" type="datatypes:QuantityForPrescribed"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
        <xsd:annotation>
          <xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="WrittenDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 85.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LastFillDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Last fill date may contain the last known fill date if the requesting pharmacy is leveraging information from the prescription label that was presented by the patient/caregiver.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 36.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 07.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value  PE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="DateValidated" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Substitutions" type="ecl:SubstitutionCodeRestricted">
        <xsd:annotation>
          <xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="2" minOccurs="0" name="Diagnosis" type="structures:DiagnosisRestricted"/>
      <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Note" type="datatypes:an1..210">
        <xsd:annotation>
          <xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
        <xsd:annotation>
          <xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
        <xsd:annotation>
          <xsd:documentation>See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType"/>
      <xsd:element minOccurs="0" name="Sig" type="structures:Sig">
        <xsd:annotation>
          <xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="Compound" type="datatypes:CompoundIngredientLotNotUsed">
        <xsd:annotation>
          <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="SoldDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
        <xsd:annotation>
          <xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
        <xsd:annotation>
          <xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
        <xsd:annotation>
          <xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
        <xsd:annotation>
          <xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="Pharmacy" type="structures:OptionalPharmacy"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="Prescriber" type="structures:HistoryPrescriberChoice"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
        <xsd:annotation>
          <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
        <xsd:annotation>
          <xsd:documentation>The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>Name of the manufacturer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="RxFillIndicator" type="ecl:RxFillIndicatorType"/>
      <xsd:element minOccurs="0" name="Service" type="structures:ServiceStructure"/>
      <xsd:element minOccurs="0" name="IVAdministration" type="structures:IVAdministrationType"/>
      <xsd:element minOccurs="0" name="SelfAdministrationAllowed" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AnticipatedDischargeDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CurrentTreatmentCycle" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NumberOfCyclesPlanned" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NextQuestionConditionType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NextQuestionConditionType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NextQuestionCondition" type="structures:NumericType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NonHumanPatient  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NonHumanPatient">
    <xsd:sequence>
      <xsd:element maxOccurs="0" minOccurs="0" name="Identification" type="datatypes:PatientID"/>
      <xsd:element name="Name" type="datatypes:NonHumanNameType"/>
      <xsd:element name="Gender" type="ecl:GenderCode"/>
      <xsd:element name="DateOfBirth" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>DateOfBirth – is mandatory; if not known, send the best estimated valid date.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="PatientLocation" type="structures:PatientLocation"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="LanguageNameCode" type="datatypes:an1..3"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="ResidenceCode" type="ecl:ResidenceCode"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="PregnancyIndicator" type="ecl:BooleanCode"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="Race" type="datatypes:CDCCode"/>
      <xsd:element maxOccurs="0" minOccurs="0" name="Ethnicity" type="datatypes:CDCCode"/>
      <xsd:element name="Species" type="datatypes:SNOMEDCodeTextMandatory">
        <xsd:annotation>
          <xsd:documentation>Text must derive from the Code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AlternateContact" type="structures:AlternateContactStructure"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NumericChoiceForPARequest  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NumericChoiceForPARequest">
    <xsd:complexContent>
      <xsd:extension base="structures:NumericTypeBasic">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="Answer" type="structures:CompletedAnswerChoiceForNumericType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NumericType  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NumericType">
    <xsd:choice>
      <xsd:element name="SingleComparison" type="structures:NumericValueSimpleType"/>
      <xsd:element name="RangeComparison" type="structures:NumericValueType"/>
    </xsd:choice>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NumericTypeBasic  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NumericTypeBasic">
    <xsd:sequence>
      <xsd:element default="Y" name="IsNumeric" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>A value of "Y" to indicate the answer to the question is Numeric. A value of "N" should not be used. Use other question type if not Numeric.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NextQuestionCondition" type="structures:NumericType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NumericValueSimpleType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NumericValueSimpleType">
    <xsd:sequence>
      <xsd:element name="ComparisonOperator" type="ecl:ComparisonOperatorCode">
        <xsd:annotation>
          <xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined boundary.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ComparisonValue" type="datatypes:n1..18">
        <xsd:annotation>
          <xsd:documentation>Value for the boundary of the comparison.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NextQuestionID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>A QuestionID or value of "END" to identify the next question when it is dependent on the answer to the question (i.e., answer choice or numeric condition). Required when the DefaultNextQuestionID for the question is not provided.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  NumericValueType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="NumericValueType">
    <xsd:sequence>
      <xsd:element name="LowerBoundComparisonValue" type="datatypes:n1..18">
        <xsd:annotation>
          <xsd:documentation>Value for the lower boundary of the comparison.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LowerBoundComparisonOperator" type="ecl:ComparisonOperatorCodeRestricted">
        <xsd:annotation>
          <xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined lower boundary</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="UpperBoundComparisonValue" type="datatypes:n1..18">
        <xsd:annotation>
          <xsd:documentation>Value for the upper boundary of the comparison.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="UpperBoundComparisonOperator" type="ecl:ComparisonOperatorCodeRestricted">
        <xsd:annotation>
          <xsd:documentation>Code that conveys the relationship between the answered value to a question and a defined upper boundary.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NextQuestionID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>A QuestionID or value of "END" to identify the next question when it is dependent on the answer to the question (i.e., answer choice or numeric condition). Required when the DefaultNextQuestionID for the question is not provided.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Observation  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Observation">
    <xsd:sequence>
      <xsd:element maxOccurs="10" minOccurs="0" name="Measurement" type="datatypes:MeasurementType"/>
      <xsd:element minOccurs="0" name="ObservationNotes" type="datatypes:an1..140"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  OptionalMedication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="OptionalMedication">
    <xsd:annotation>
      <xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DrugDescription" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Quantity" type="datatypes:QuantityAbstract">
        <xsd:annotation>
          <xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
        <xsd:annotation>
          <xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="WrittenDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 85.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LastFillDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value LD.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 36.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 07.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value  PE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DateValidated" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Substitutions" type="ecl:SubstitutionCodeRestricted">
        <xsd:annotation>
          <xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Diagnosis" type="structures:Diagnosis"/>
      <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Note" type="datatypes:an1..210">
        <xsd:annotation>
          <xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
        <xsd:annotation>
          <xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
        <xsd:annotation>
          <xsd:documentation>See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Sig" type="structures:Sig">
        <xsd:annotation>
          <xsd:documentation>See Medication for Definition and Usage.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Compound" type="datatypes:CompoundIngredient">
        <xsd:annotation>
          <xsd:documentation>For ingredient information. Sub elements loop for each ingredient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="SoldDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
        <xsd:annotation>
          <xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
        <xsd:annotation>
          <xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
        <xsd:annotation>
          <xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
        <xsd:annotation>
          <xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Pharmacy" type="structures:OptionalPharmacy"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Prescriber" type="structures:HistoryPrescriberChoice"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment"/>
      <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
        <xsd:annotation>
          <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
        <xsd:annotation>
          <xsd:documentation>The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>Name of the manufacturer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="0" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
      <xsd:element minOccurs="0" name="Service" type="structures:ServiceStructure"/>
      <xsd:element minOccurs="0" name="IVAdministration" type="structures:IVAdministrationType"/>
      <xsd:element minOccurs="0" name="SelfAdministrationAllowed" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AnticipatedDischargeDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CurrentTreatmentCycle" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NumberOfCyclesPlanned" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  OptionalPharmacy  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="OptionalPharmacy">
    <xsd:annotation>
      <xsd:documentation>Value P2</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Identification" type="datatypes:PharmacyID">
        <xsd:annotation>
          <xsd:documentation>On the mandatory transactions: One occurrence must contain the value “D3” (NCPDP Provider ID Number). One occurrence must contain the value "HPI” (National Provider ID)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
        <xsd:annotation>
          <xsd:documentation>Value A.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Pharmacist" type="structures:Pharmacist"/>
      <xsd:element name="BusinessName" type="datatypes:an1..70"/>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  OptionalPrescriber  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="OptionalPrescriber">
    <xsd:annotation>
      <xsd:documentation>Value PC</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:HistoryPrescriber">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:NonVeterinarianID">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
            <xsd:annotation>
              <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PrescriberAgent" type="structures:NameComposite"/>
          <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See HistoryPrescriber for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  OptionalPrescriberChoice  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="OptionalPrescriberChoice">
    <xsd:complexContent>
      <xsd:restriction base="structures:HistoryPrescriberChoice">
        <xsd:choice>
          <xsd:element name="NonVeterinarian" type="structures:OptionalPrescriber"/>
          <xsd:element name="Veterinarian" type="structures:VetPrescriberMirrorOptionalPrescriber"/>
        </xsd:choice>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  OrderGroup  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="OrderGroup">
    <xsd:sequence>
      <xsd:element name="OrderGroupNumber" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>Unique ID assigned by the prescriber/pharmacy to link multiple medication orders together.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PositionInOrderGroup" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Indicates the position of the medication order within the group.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TotalCountForOrderGroup" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Indicates the total number of medication orders within the group.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  OtherCompoundInformationType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="OtherCompoundInformationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CompoundCoded" type="ecl:CompoundCode"/>
      <xsd:element minOccurs="0" name="FinalCompoundPharmaceuticalDosageForm" type="datatypes:NCICode">
        <xsd:annotation>
          <xsd:documentation>NCPDP Drug StrengthForm Terminology - available at http://www.cancer.gov/cancertopics/terminologyresources/page7. See External Code List Introduction for information on NCI Thesaurus Code Lists.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PAAppeal  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PAAppeal">
    <xsd:sequence>
      <xsd:element name="IsEAppealSupported" type="ecl:BooleanCode">
        <xsd:annotation>
          <xsd:documentation>Indicates if payer supports electronic appeals submission.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExpirationDate" type="datatypes:DateType"/>
      <xsd:element minOccurs="0" name="PANote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PAAppealRequestType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PAAppealRequestType">
    <xsd:sequence>
      <xsd:element name="PACaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AppealCaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the payer to identify the specific appeal request. If a AppealCaseID is assigned, it is to be exchanged on all subsequent appeal transactions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DeadlineForReply" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>The timeframe for the provider to provide requested information. This element is related to the case and contains the expiration date that the prescriber is to take the next step. It is not the expiration date of the questions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="QuestionSet" type="structures:QuestionSetGroupForRequest"/>
      <xsd:element minOccurs="0" name="PANote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PAApprovedDetail  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PAApprovedDetail">
    <xsd:sequence>
      <xsd:element name="PACaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AuthorizationNumber" type="datatypes:an1..140"/>
      <xsd:element minOccurs="0" name="AuthorizationPeriod" type="structures:DateGroupWithEffectiveExpiredDate"/>
      <xsd:element name="Appeal" type="structures:PAAppeal"/>
      <xsd:element minOccurs="0" name="PANote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PAProcessor" type="structures:PAProcessor"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PAApprovedDetailForAppeal  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PAApprovedDetailForAppeal">
    <xsd:sequence>
      <xsd:element name="PACaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the payer to identify the specific case. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AuthorizationNumber" type="datatypes:an1..140"/>
      <xsd:element minOccurs="0" name="AuthorizationPeriod" type="structures:DateGroupWithEffectiveExpiredDate"/>
      <xsd:element name="Appeal" type="structures:PAAppeal"/>
      <xsd:element minOccurs="0" name="PANote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AppealCaseID" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>ID assigned by the payer to identify the specific appeal request. If a AppealCaseID is assigned, it is to be exchanged on all subsequent appeal transactions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PAApprovedForCancel  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PAApprovedForCancel">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PANote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  PAAuthorizationDetails  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="PAAuthorizationDetails">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="PharmacyType" type="ecl:PharmacyType"/>
      <xsd:element minOccurs="0" name="Quantity" type="datatypes:QuantityAbstract"/>
      <xsd:element minOccurs="0" name="DaysSupply" type="datatypes:n1..3"/>
      <xsd:element minOccurs="0" name="NumberOfCycles" type="datatypes:n1..3">
        <xsd:annotation>
          <xsd:documentation>Number of cycles authorized.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2"/>
      <xsd:element minOccurs="0" name="PANote" type="datatypes:an1..2000">
        <xsd:annotation>
          <xsd:documentation>Free text note provides additional information regarding the response status.</xsd:documentation>
