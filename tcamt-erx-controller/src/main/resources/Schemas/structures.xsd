    <xsd:choice>
      <xsd:sequence>
        <xsd:choice>
          <xsd:element name="MeasurementAdministrationTiming" type="structures:MeasurementTiming"/>
          <xsd:element name="MeasurementFrequency" type="structures:MeasurementFrequencyTiming"/>
          <xsd:element name="MeasurementInterval" type="structures:MeasurementIntervalTiming"/>
        </xsd:choice>
      </xsd:sequence>
      <xsd:element name="MeasurementDuration" type="structures:MeasurementDuration"/>
