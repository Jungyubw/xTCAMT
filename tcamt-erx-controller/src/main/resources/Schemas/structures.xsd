            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Pharmacy" type="structures:Pharmacy">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="Prescriber" type="structures:PrescriberGeneralChoice"/>
          <xsd:element maxOccurs="0" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment">
            <xsd:annotation>
              <xsd:documentation>See Medication for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
            <xsd:annotation>
              <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
            <xsd:annotation>
              <xsd:documentation>The prescription has been split for both mail order and retail. The retail order is only for a limited supply until the mail order arrives.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
            <xsd:annotation>
              <xsd:documentation>Name of the manufacturer.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element maxOccurs="0" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
          <xsd:element name="RxFillIndicator" type="ecl:RxFillIndicatorType"/>
          <xsd:element minOccurs="0" name="Service" type="structures:ServiceStructure"/>
          <xsd:element minOccurs="0" name="IVAdministration" type="structures:IVAdministrationType"/>
          <xsd:element minOccurs="0" name="SelfAdministrationAllowed" type="ecl:BooleanCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if patient is allowed to self-administer medication.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="TreatmentIndicator" type="ecl:TreatmentIndicatorCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if the order is new, continuation of treatment or a restart of treatment.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ProphylacticOrEpisodic" type="ecl:ProphylacticOrEpisodicCode">
            <xsd:annotation>
              <xsd:documentation>Indicates if treatment is for Prophylactic and/or episodic needs.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="AnticipatedDischargeDate" type="datatypes:DateType">
            <xsd:annotation>
              <xsd:documentation>Indicates expected date of patient's discharge from facility.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CurrentTreatmentCycle" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Current treatment cycle represented by this prescription.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="NumberOfCyclesPlanned" type="datatypes:n1..2">
            <xsd:annotation>
              <xsd:documentation>Number of treatment cycles planned.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  RxHistoryBenefitsCoordination  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="RxHistoryBenefitsCoordination">
    <xsd:complexContent>
      <xsd:restriction base="structures:BaseBenefitsCoordination">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="PayerIdentification" type="datatypes:PayerID">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PayerName" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CardholderID" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CardHolderName" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GroupID" type="datatypes:an1..35">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PayerResponsibilityCode" type="ecl:PayerResponsibilityCode">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PatientRelationship" type="ecl:PatientRelationshipCode">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PersonCode" type="datatypes:n1..3">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="GroupName" type="datatypes:an1..70">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Consent" type="ecl:ConsentCode">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PBMMemberID" type="datatypes:an1..80">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ResponsibleParty" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="ServiceTypeCoded" type="ecl:ServiceTypeCode">
            <xsd:annotation>
              <xsd:documentation>See BaseBenefitsCoordination for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ScoreType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="ScoreType">
    <xsd:sequence>
      <xsd:element name="ScoreSource" type="datatypes:an1..255">
        <xsd:annotation>
          <xsd:documentation>Indicates the product or organization generating the associated score and report.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ReportURL" type="datatypes:an1..1000">
        <xsd:annotation>
          <xsd:documentation>URL where an additional report with the SCOREValue can be requested.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence maxOccurs="unbounded">
        <xsd:element minOccurs="0" name="ScoreName" type="datatypes:an1..255">
          <xsd:annotation>
            <xsd:documentation>Descriptive name of score.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ScoreValue" type="datatypes:an1..10">
          <xsd:annotation>
            <xsd:documentation>Value of score.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SelectTypeForRequest  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SelectTypeForRequest">
    <xsd:complexContent>
      <xsd:extension base="structures:PASelectType">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Answer" type="structures:CompletedAnswerChoiceForSelectType"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ServiceStructure  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="ServiceStructure">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="AgencyOfService" type="structures:AgencyOfServiceType"/>
      <xsd:element minOccurs="0" name="ServiceType" type="structures:ServiceType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  ServiceType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="ServiceType">
    <xsd:annotation>
      <xsd:documentation>Used in MTMServiceRequest, MTMServiceResponse</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:element name="TypeOfServiceFreeText" type="datatypes:an">
          <xsd:annotation>
            <xsd:documentation>Can send FreeText or Code/Qualifier/Text - but not both. If a code list exists, the Code/Qualifier/Text are used, where Text must be from the Code Source, not free text.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="TypeOfService" type="datatypes:QualifiedCodeWithText">
          <xsd:annotation>
            <xsd:documentation>Note: The codification for MTM services is underway. At this point, to exchange the Service, the industry will exchange free text (TypeOfServiceFreeText) until services are codified. TypeOfServiceFreeText may be used or (TypeOfServiceCodeQualifier, TypeOfServiceCode and TypeOfServiceCodeText) – but not both. Once codified, TypeOfServiceCodeText, TypeOfServiceCode, TypeOfServiceCodeQualifier will be used and TypeOfServiceFreeText will be sunsetted.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:choice minOccurs="0">
        <xsd:element name="TargetedTypeOfServiceFreeText" type="datatypes:an">
          <xsd:annotation>
            <xsd:documentation>Can send FreeText or Code/Qualifier/Text - but not both. If a code list exists, the Code/Qualifier/Text are used, where Text must be from the Code Source, not free text</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="TargetedTypeOfService" type="datatypes:QualifiedCodeWithText">
          <xsd:annotation>
            <xsd:documentation>Note: The TargetedTypeOfServiceCodeQualifier contains code lists from which a codified TargetedTypeOfServiceCode and TargetedTypeOfServiceCodeText are to be populated. However, if a code does not exist for the target, free text may be used in the TargetedTypeOfServiceFreeText. TargetedTypeOfServiceFreeText may be used or (TargetedTypeOfServiceCodeQualifier, TargetedTypeOfServiceCode and TargetedTypeOfServiceCodeText) – but not both.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:element minOccurs="0" name="EffectiveDate" type="datatypes:DateType"/>
      <xsd:element minOccurs="0" name="ExpirationDate" type="datatypes:DateType"/>
      <xsd:choice minOccurs="0">
        <xsd:annotation>
          <xsd:documentation> Can send FreeText or Code/Qualifier/Text - but not both. If a code list exists, the Code/Qualifier/Text are used, where Text must be from the Code Source, not free text. Note: The codification for MTM services is underway. At this point, to exchange the Reason, the industry will exchange free text (ReasonForMTMServiceFreeText) until actions are codified. ReasonForMTMServiceFreeText may be used or (ReasonForMTMServiceQualifier, ReasonForMTMServiceText and ReasonForMTMServiceCode) but not both. Once codified, ReasonForMTMServiceText, ReasonForMTMServiceCode, ReasonForMTMServiceQualifier will be used and ReasonForMTMServiceFreeText will be sunsetted.</xsd:documentation>
        </xsd:annotation>
        <xsd:element name="ReasonForMTMServiceFreeText" type="datatypes:an"/>
        <xsd:element name="ReasonForMTMService" type="datatypes:QualifiedCodeWithText"/>
      </xsd:choice>
      <xsd:element default="Y" minOccurs="0" name="TypeOfServiceGroupSetting" type="datatypes:an"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Sig  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Sig">
    <xsd:sequence>
      <xsd:element name="SigText" type="datatypes:an1..1000">
        <xsd:annotation>
          <xsd:documentation>SigText contains either 1) completely free text with no corresponding structured content OR 2) This is a generated structured English version of the codified sig content that follows the sig grammar in the XML. The SigText should have no more and no less then what is in the structured Sig content. If you can not directly generate the SigText from the Structured Sig, do not send the structured Sig. If the structured Sig exists, the SigText must be generated directly from Structured Sig. If this is not possible for whatever reason, do not send the structured Sig.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Structured Sig is the preferred but if for some reason you can not send the structured Sig, use the SigText.</xsd:documentation>
        </xsd:annotation>
        <xsd:element name="CodeSystem">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="SNOMEDVersion" type="datatypes:an"/>
              <xsd:element name="FMTVersion" type="datatypes:an">
                <xsd:annotation>
                  <xsd:documentation>If using structured Sig but not needing to send any of the elements that use dose form, send a valid FMT version using by the application for other sigs.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="Instruction" type="structures:Instruction">
          <xsd:annotation>
            <xsd:documentation>Instruction is the focal point of the Structured Sig - An Instruction is defined as Dose Administration with one to many associated Timing and/or Durations</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:sequence maxOccurs="49" minOccurs="0">
          <xsd:element name="MultipleInstructionModifier" type="datatypes:ANDORTHENNOT"/>
          <xsd:element name="Instruction" type="structures:Instruction"/>
        </xsd:sequence>
        <xsd:sequence maxOccurs="50" minOccurs="0">
          <xsd:element name="IndicationForUse" type="structures:Indication"/>
        </xsd:sequence>
        <xsd:element maxOccurs="50" minOccurs="0" name="MaximumDoseRestriction" type="structures:MaximumDoseRestriction"/>
        <xsd:element minOccurs="0" name="ClarifyingFreeText" type="datatypes:an1..255">
          <xsd:annotation>
            <xsd:documentation>See SCRIPT Implementation guide for proper use of clarifying free text elements and code sets</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SpecializedMedication  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SpecializedMedication">
    <xsd:annotation>
      <xsd:documentation>Prescribed or Dispensed may only occur once, according to appropriate need in a transaction. A transaction may contain a loop of Prescribed and Dispensed for example, in the RxChangeRequest, where the prescriber prescribed x and the pharmacy filled y. But a transaction may not contain multiple Prescribed and Dispensed.

</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="DrugDescription" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>The appropriate source data element should contain the description from the commercially available product name (or the name that appeared when it was commercially available). It may generally contain the drug name, strength unit, and form, as appropriate. If CompoundCoded = 2 (Compound), this field must contain “0” (zero means zero).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoded" type="datatypes:DrugCoded"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Quantity" type="datatypes:QuantityAbstract">
        <xsd:annotation>
          <xsd:documentation>For multi-ingredient compounds will contain final quantity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DaysSupply" type="datatypes:n1..3">
        <xsd:annotation>
          <xsd:documentation>Note: It is strongly recommended that DaysSupply (value “ZDS”) be supported.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="WrittenDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 85.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LastFillDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value LD.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ExpirationDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 36.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="EffectiveDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value 07.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PeriodEnd" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value  PE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DeliveredOnDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DateValidated" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value BE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Substitutions" type="ecl:SubstitutionCode">
        <xsd:annotation>
          <xsd:documentation>Note: Value 0 = No Product Selection Indicated. This value should be used to designate that substitutions are allowed.  When a prescriber wishes to allow substitutes for a NewRx, this field should be 0 to reflect that substitutions can be allowed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NumberOfRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value R. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Diagnosis" type="structures:DiagnosisRestricted"/>
      <xsd:element minOccurs="0" name="CouponNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorization" type="datatypes:an1..35">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Note" type="datatypes:an1..210">
        <xsd:annotation>
          <xsd:documentation>If CompoundCoded = 2 (Compound), this field is to be used when special instructions are needed.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugUseEvaluation" type="datatypes:DrugUseEvaluation">
        <xsd:annotation>
          <xsd:documentation>For further explanation, conflict, or clarification of services related to drug use evaluation.           
When this composite is used, ServiceReasonCode is mandatory. 
When the ServiceReasonCode is sent from the prescriber to the pharmacist, the ServiceResultCode is mandatory.
When the ServiceReasonCode is sent from the pharmacist to the prescriber, the ServiceResultCode is conditional.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DrugCoverageStatusCode" type="ecl:DrugCoverageStatusCode">
        <xsd:annotation>
          <xsd:documentation>See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PriorAuthorizationStatus" type="ecl:PriorAuthorizationStatusCode">
        <xsd:annotation>
          <xsd:documentation>The status of the prescription’s prior authorization as known by the sender.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OtherCompoundInformation" type="structures:OtherCompoundInformationType"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Sig" type="structures:Sig">
        <xsd:annotation>
          <xsd:documentation>The Structured SIG element is used to codify the SIG information. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Compound" type="datatypes:CompoundIngredient">
        <xsd:annotation>
          <xsd:documentation>For ingredient information. Sub elements loop for each ingredient.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="SoldDate" type="datatypes:DateType">
        <xsd:annotation>
          <xsd:documentation>Value “06” (Sold Date) is only allowed to be used in RxHistoryResponse, RxFill, and Refill transactions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="PharmacyRequestedRefills" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value P. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="DoNotFill" type="ecl:DoNotFillCode">
        <xsd:annotation>
          <xsd:documentation>Used for medications ordered by a prescriber but not requiring dispensing at this time, but may be required for administration and may be available for drug-to-drug interactions.
If the prescriber knows the patient does not need the prescription filled right away, then they should send the pharmacy this indicator</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanDate" type="datatypes:UtcDateType">
        <xsd:annotation>
          <xsd:documentation>For the facility to relay to the long term care pharmacy the timeframe when medication is needed for delivery.
When a facility transmits either a new medication order (NEWRX) or request for the re-supply of a medication (RESUPPLY) to a pharmacy, it would optionally indicate the time by which the medication is needed. The facility could also provide a textual reason why the medication is needed by the time specified.

The long term care pharmacy would then use this information to determine whether a special delivery is required, or whether the order could go out with the next scheduled delivery.
For NewRx or Resupply in long term care setting, only value supported - 203 = CCYYMMDDHHMM; C=Century; Y=Year; M=Month; D=Day; H=Hour; M=Minutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="TimeZone" type="datatypes:TimeZone">
        <xsd:annotation>
          <xsd:documentation>Defines the time zone used by the sender. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="NeededNoLaterThanReason" type="datatypes:an1..70"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="RefillsRemaining" type="datatypes:n1..2">
        <xsd:annotation>
          <xsd:documentation>Value REM. See Imp Guide for more information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistoryPrescriberOrderNumber" type="datatypes:an1..35"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="OrderCaptureMethod" type="ecl:OrderCaptureMethodCode">
        <xsd:annotation>
          <xsd:documentation>Code conveying the method by which the order was defined by the prescriber and captured in the prescribing system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Pharmacy" type="structures:OptionalPharmacy"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Prescriber" type="structures:HistoryPrescriberChoice"/>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="HistorySource" type="datatypes:HistorySourceWithoutPayment"/>
      <xsd:element minOccurs="0" name="ReasonForSubstitutionCodeUsed" type="ecl:ReasonForSubstitutionCodeUsed">
        <xsd:annotation>
          <xsd:documentation>Restricted text for submitter to define their clarification basis for Substitution code applied. If Substitutions = 1, ReasonForSubstitutionCodeUsed must contain "Brand Medically Necessary" (for all Medicaid programs).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SplitScript" type="ecl:SplitScriptIndicator">
        <xsd:annotation>
          <xsd:documentation> The prescription has been split for both mail order and retail. The retail order is only for a limited supply  until the mail order arrives.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="ManufacturerName" type="datatypes:an1..105">
        <xsd:annotation>
          <xsd:documentation>Name of the manufacturer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="LotNumber" type="datatypes:LotNumber"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SubstanceUseType  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SubstanceUseType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="Substance" type="structures:Substances"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Substances  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Substances">
    <xsd:sequence>
      <xsd:element name="Type" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="Level" type="datatypes:SigCode"/>
      <xsd:element minOccurs="0" name="RouteOfAdministration" type="datatypes:SigCode"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  Supervisor  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="Supervisor">
    <xsd:annotation>
      <xsd:documentation>To describe the supervisor. Supervisor information may be needed based upon state law. A supervisor is the supervising physician under whose authority the prescription is being prescribed by the prescriber. 

Value SU</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:SupervisorOptional">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:SupervisorID">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
            <xsd:annotation>
              <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorMandatoryAddress  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorMandatoryAddress">
    <xsd:annotation>
      <xsd:documentation>Value SU</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="structures:Supervisor">
        <xsd:sequence>
          <xsd:element name="Identification" type="datatypes:SupervisorID">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
            <xsd:annotation>
              <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Name" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
            <xsd:annotation>
              <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="Address" type="datatypes:MandatoryAddressType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType">
            <xsd:annotation>
              <xsd:documentation>See OptionalSupervisor for Definition and Usage</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorMandatoryAddressType  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorMandatoryAddressType">
    <xsd:complexContent>
      <xsd:restriction base="structures:SupervisorTypeAbstract">
        <xsd:choice>
          <xsd:element name="NonVeterinarian" type="structures:SupervisorMandatoryAddress"/>
          <xsd:element name="Veterinarian" type="structures:VetSupervisorMirrorSupervisorMandatoryAddress"/>
        </xsd:choice>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorOptional  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorOptional">
    <xsd:annotation>
      <xsd:documentation>To describe the supervisor. Supervisor information may be needed based upon state law. A supervisor is the supervising physician under whose authority the prescription is being prescribed by the prescriber. </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Identification" type="datatypes:SupervisorID"/>
      <xsd:element minOccurs="0" name="Specialty" type="datatypes:an1..10"/>
      <xsd:element minOccurs="0" name="PracticeLocation" type="structures:PracticeLocationType">
        <xsd:annotation>
          <xsd:documentation>The identity of the organization the practitioner represents or acts on behalf of.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Name" type="datatypes:NameType"/>
      <xsd:element minOccurs="0" name="FormerName" type="datatypes:NameType">
        <xsd:annotation>
          <xsd:documentation>Used to provide additional matching criteria when the current Name does not match</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Address" type="datatypes:AddressType"/>
      <xsd:element name="CommunicationNumbers" type="datatypes:CommunicationNumbersType"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorOptionalType  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorOptionalType">
    <xsd:complexContent>
      <xsd:restriction base="structures:SupervisorTypeAbstract">
        <xsd:choice>
          <xsd:element name="NonVeterinarian" type="structures:SupervisorOptional"/>
          <xsd:element name="Veterinarian" type="structures:VetSupervisorMirrorSupervisorOptional"/>
        </xsd:choice>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorType  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <xsd:complexType name="SupervisorType">
    <xsd:complexContent>
      <xsd:restriction base="structures:SupervisorTypeAbstract">
        <xsd:choice>
          <xsd:element name="NonVeterinarian" type="structures:Supervisor"/>
          <xsd:element name="Veterinarian" type="structures:VetSupervisorMirrorSupervisor"/>
        </xsd:choice>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
  <!--  SupervisorTypeAbstract  <<choice>>  -->
  <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
